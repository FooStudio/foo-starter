{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": [
      "react"
    ],
    "globals": {
        "App": true,
        "System": true,
        "$t": true,
        "PIXI": true,
        "Map": true,
        "TweenMax": true,
        "TweenLite": true,
        "TimelineMax": true,
        "TimelineLite": true,
        "Sine": true,
        "Power1": true,
        "Power2": true,
        "Power3": true,
        "Power4": true,
        "Back": true,
        "Elastic": true,
        "Linear": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
      "no-console": 0,
      "max-len": 0,
      "comma-dangle": 0,
      "arrow-body-style": 0,
      "react/prefer-stateless-function": 0,
      "react/jsx-boolean-value": 0,
      "react/prop-types": 0,
    },
}
